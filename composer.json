{
  "name": "dsentker/url-fingerprint",
  "type": "library",
  "description": "Build a hash from a url",
  "keywords": [
    "url",
    "uri",
    "rfc3986",
    "rfc3987",
    "rfc6570",
    "psr-7",
    "parse_url",
    "http",
    "https",
    "parse_str",
    "query-string",
    "querystring",
    "hostname",
    "fingerprinting"
  ],
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/dsentker/urlhasher",
  "authors": [
    {
      "name": "Daniel Sentker",
      "email": "tracid2008@gmail.com",
      "homepage": "https://github.com/dsentker"
    }
  ],
  "require": {
    "php": ">=7.4",
    "league/uri": "^6.0",
    "league/uri-components": "^2.3",
    "symfony/options-resolver": "^5.3",
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0 || ^9.0"
  },
  "autoload": {
    "psr-4": {
      "UrlFingerprint\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UrlFingerprintTest\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  }
}
